{
	"name": "FacebookZucks",
	"version": "0.1",
	"description": "",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {

		}
	},
	"icons": {

	},
	"permissions": [
		"activeTab"
	],
	"content_scripts": [
		{
			"matches": ["*://*.facebook.com/*"],
			"run_at": "document_idle",
			"js": ["bin/main.js"]
		}
	]
}